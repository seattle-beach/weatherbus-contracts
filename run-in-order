#!/bin/sh -e

# Assumes that golden-gardens is in ../golden-gardens and there is a freshly restarted
# weatherbus with an empty database at http://localhost:8080.

for c in addUser addStop stops-for-user stop; do
  echo "Trying $c"
  ../golden-gardens/test-service "${c}/contract.json" http://localhost:8080
done

echo "All passed."
